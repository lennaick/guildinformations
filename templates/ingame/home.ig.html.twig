{% extends "ingame/content.ig.html.twig" %}

{% block title %}
	{{ parent() }} - {{ translate('m.home.title') }}
{% endblock title %}

{% block content %}
	{% if grade=="NoGuild" %}
		<div>
			<p>{{ translate('m.home.noGuild') }}<br></p>
			<p>{{ translate('m.home.backTo') }} <a href="http://app.ryzom.com">Ryzom</a>.<br></p>
		</div>
	{% else %}
		<h2>{{ translate('m.home.welcome') }}</h2>
	{% endif %}
{% endblock content %}