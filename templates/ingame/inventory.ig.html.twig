{% extends "ingame/inventoryHome.ig.html.twig" %}

{% block inventoryContent %}
	<h2>{{ guildName }}</h2>
	<div>
	{% if items.error %}
		<p>Problème de clé</p>
	{% else %}
		{% for item in items %}
			<img src="{{ item.iconUrl }}" style="border: 1px solid #333;" title="{{ item.stack }}x{{ item.name }} (Q{{ item.quality }}{{ item.details }})"/>
		{% endfor %}
	{% endif %}
	</div>
{% endblock %}