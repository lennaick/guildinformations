{% block inventoryMenu %}
	<div class="padding black">
		<span>
			<span>
				<a href="{{ urlFor('ryzomApp-Inventory') }}?checksum={{ checksum }}&user={{ user }}">Chercher un objet</a>
			</span>
			<span>
				 | Afficher:
			{% for guild in guilds %}
				<span>
					 - <a href="{{ urlFor('ryzomApp-Inventory/guild', {'guildId': guild.id}) }}?checksum={{ checksum }}&user={{ user }}">{{ guild.name }}</a>
				</span>
			{% endfor %}
			</span>
		</span>
	</div>
{% endblock %}