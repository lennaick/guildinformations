{% extends "content.html.twig" %}

{% block title %}
	{{ parent() }} - {{ translate('m.home.title') }}
{% endblock title %}

{% block pageTitle %}{{ translate('m.home.title') }}{% endblock pageTitle %}

{% block content %}
	{% if grade=="NoGuild" %}
		<div>
			<p>{{ translate('m.home.noGuild') }}</p>
			<p>{{ translate('m.home.backTo') }} <a href="http://app.ryzom.com">Ryzom</a>.</p>
		</div>
	{% else %}
		<div><h2>{{ translate('m.home.welcome') }}</h2></div>
	{% endif %}
{% endblock content %}