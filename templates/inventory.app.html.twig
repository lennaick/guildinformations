{% extends "content.html.twig" %}

{% block content %}
	<a href="{{ urlFor('ryzomApp-Home') }}?checksum={{ checksum }}&user={{ user }}">Retour accueil</a>
	{% for item in items %}
		<img src="{{ item.iconUrl }}" style="border: 1px solid #333;" title="{{ item.stack }}x{{ item.name }} (Q{{ item.quality }})"/>
	{% endfor %}
{% endblock %}